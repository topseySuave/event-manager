{
  "name": "boots-event-manager",
  "version": "1.0.0",
  "description": "An Event Manager application to create and manage your own event.",
  "main": "./server/app.js",
  "engines": {
    "node": "9.11.1",
    "npm": "5.6.0"
  },
  "scripts": {
    "build": "npm run clean && npm run build:server && npm run build:client",
    "clean": "rm -rf build && mkdir build",
    "babel-node": "babel-node --preset=es2015",
    "start": "npm run clean && npm run build:server && node -- ./build/app.js",
    "start:dev": "npm run start:client & nodemon --exec babel-node -- ./server/app.js",
    "build:client": "webpack --config webpack.prod.js",
    "build:server": "babel -d ./build --copy-files ./server -s",
    "start:client": "npm run webpack",
    "start:server": "nodemon --exec babel-node -- ./server/app.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "pretest": "NODE_ENV=test npm run drop:db && npm run create:db && npm run migrate:test",
    "test:local": "npm run pretest && npm run test:server",
    "test": "npm run pretest && npm run test:server && npm run test:client",
    "test:client": "nyc --reporter=html --reporter=text jest -u --maxWorkers=4 && codecov",
    "test:server": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --timeout 20000 --exit --require babel-core/register server/test/index.js",
    "test:dev:server": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha ./server/test/index.js --exit --require babel-node/register --reporter spec",
    "test:dev": "npm test && npm run coverage",
    "webpack": "webpack --config webpack.dev.js --watch",
    "migrate": "sequelize db:migrate",
    "seed": "sequelize db:seed",
    "drop:test": "sequelize db:migrate:undo:all --env test",
    "migrate:test": "sequelize db:migrate --env test",
    "drop:db": "sequelize db:drop boots_test --env test",
    "create:db": "sequelize db:create boots_test --env test",
    "e2e": "nightwatch"
  },
  "nyc": {
    "exclude": [
      "server/middleware/imageUploader.js",
      "server/middleware/mailer.js",
      "server/models/**",
      "server/controllers/**",
      "server/app.js",
      "server/routes/administrator.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/topseysuave/event-manager.git"
  },
  "author": "Gabriel Micah",
  "license": "ISC",
  "bug": {
    "url": "https://github.com/topseysuave/event-manager/issues"
  },
  "homepage": "https://github.com/topseysuave/event-manager#readme",
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ]
  },
  "jest": {
    "verbose": true,
    "testRegex": "client/__tests__/.*\\.test\\.(js|jsx)$",
    "setupTestFrameworkScriptFile": "./client/__tests__/setup.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/**",
      "!client/src/index.jsx",
      "!client/src/components/modals/centerModalForms/*.*",
      "!client/src/components/modals/eventModalForm/*.*"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/server/",
      "/build/",
      "/client/src/registerServiceWorker.js",
      "/client/src/rootReducer.js",
      "/client/src/util/",
      "/client/src/actions/modalAction.js",
      "/client/src/actions/searchAction.js",
      "/client/src/actions/events-actions/index.js",
      "/client/src/helpers/authCheck.js"
    ],
    "testPathIgnorePatterns": [
      "/client/__tests__/__mocks__/"
    ]
  },
  "dependencies": {
    "animate.css": "^3.6.1",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "babel-core": "*",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node5": "*",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "*",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "cloudinary": "^1.9.1",
    "compression-webpack-plugin": "^1.1.10",
    "connect-multiparty": "^2.1.0",
    "consolidate": "^0.15.0",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "faker": "*",
    "fetch-ie8": "^1.5.0",
    "file-loader": "^1.1.5",
    "history": "^4.7.2",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^3.4.2",
    "jade": "^1.11.0",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.20.0",
    "materialize-css": "^0.100.2",
    "mini-css-extract-plugin": "^0.4.0",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "multiparty": "^4.1.3",
    "node-sass": "^4.5.3",
    "nodemailer": "^4.5.0",
    "nodemon": "^1.12.1",
    "nyc": "^11.2.1",
    "open-browser-webpack-plugin": "0.0.5",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.7",
    "precss": "^2.0.0",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.0.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.0.0",
    "react-dropzone": "^4.2.1",
    "react-hot-loader": "^3.1.3",
    "react-redux": "^5.0.6",
    "react-redux-loading-bar": "^3.0.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.1.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.35.2",
    "sequelize-cli": "^4.0.0",
    "sha1": "^1.1.1",
    "shortid": "^2.2.8",
    "style-loader": "^0.19.0",
    "superagent": "^3.8.0",
    "supertest": "^3.0.0",
    "swagger-ui-express": "^2.0.11",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "uuid": "^3.1.0",
    "validator": "^9.2.0",
    "validatorjs": "^3.13.5",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.1.3",
    "webpack-config": "^7.5.0",
    "webpack-merge": "^4.1.2"
  },
  "devDependencies": {
    "babel-preset-react-hmre": "^1.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "expect": "^21.2.1",
    "faker": "^4.1.0",
    "jest": "^22.3.0",
    "jsdom": "^11.11.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "moxios": "^0.4.0",
    "react-router-enzyme-context": "^1.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.5.1",
    "webpack-dev-server": "^3.1.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
