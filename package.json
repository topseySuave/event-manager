{
  "name": "boots-event-manager",
  "version": "1.0.0",
  "description": "An Event Manager application to create and manage your own event.",
  "main": "server/app.js",
  "scripts": {
    "start": "babel-node server/app.js --compilers js:babel-register",
    "start:dev": "nodemon ./server/app.js --exec babel-node --presets es2015,stage-2",
    "build": "babel-node server/app.js",
    "test": "nyc --reporter=html --reporter=text mocha ./server/test/ --compilers js:babel-register",
    "test:dev": "./node_modules/.bin/mocha --exit --compilers js:babel-core/register server/test/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "author": "Gabriel Micah",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babel-cli": "^6.26.0",
    "babel-core": "*",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node5": "*",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "*",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^8.1.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.19.3",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nyc": "^11.2.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.11",
    "swagger-ui-express": "^2.0.11",
    "validatorjs": "^3.13.5",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "webpack-config": "^7.5.0"
  }
}
