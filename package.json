{
  "name": "boots-event-manager",
  "version": "1.0.0",
  "description": "An Event Manager application to create and manage your own event.",
  "main": "server/app.js",
  "scripts": {
    "start": "babel-node ./server/bin/www",
    "start:dev": "nodemon ./server/bin/www --exec --presets es2015,stage-2",
    "build": "babel server",
    "test": "nyc --reporter=html --reporter=text mocha ./server/test/ --compilers js:babel-register",
    "test:dev": "./node_modules/.bin/mocha --timeout 15000 --compilers js:babel-core/register server/test/*.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test"
  },
  "author": "Gabriel Micah",
  "license": "ISC",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.0.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "jquery": "^3.2.1",
    "morgan": "^1.9.0",
    "nyc": "^11.2.1",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.22.11",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "*",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-node5": "*",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "*",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.11.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.5.1",
    "faker": "^4.1.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.12.1",
    "webpack-config": "^7.5.0"
  }
}
